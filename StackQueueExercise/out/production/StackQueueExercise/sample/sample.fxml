<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" prefHeight="471.0" prefWidth="553.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ListView fx:id="listStack" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <ListView fx:id="listQueue" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="4" />
      <HBox prefHeight="21.0" prefWidth="200.0" GridPane.rowIndex="3">
         <children>
            <Button mnemonicParsing="false" onAction="#enqueue" prefHeight="25.0" prefWidth="101.0" text="EnQueue" />
            <Button mnemonicParsing="false" onAction="#dequeue" prefHeight="25.0" prefWidth="105.0" text="DeQueue" />
         </children>
      </HBox>
      <TextField fx:id="txtQueue" promptText="queue" GridPane.rowIndex="2" />
      <HBox prefHeight="27.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <children>
            <Button mnemonicParsing="false" onAction="#push" prefHeight="25.0" prefWidth="92.0" text="Push" />
            <Button mnemonicParsing="false" onAction="#pop" prefHeight="25.0" prefWidth="104.0" text="Pop" />
         </children>
      </HBox>
      <TextField fx:id="txtStack" promptText="stack" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" onAction="#load" text="Load" />
      <Button mnemonicParsing="false" onAction="#save" text="Save" GridPane.columnIndex="1" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
